{
  "name": "ember-steps",
  "version": "10.0.0",
  "description": "Ember addon for declaratively creating wizards, tabbed UIs, and more",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "release": "standard-version"
  },
  "main": "index.js",
  "homepage": "https://alexlafroscia.github.io/ember-steps",
  "engines": {
    "node": "10.* || >= 12"
  },
  "dependencies": {
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "ember-cli-babel": "^7.20.5",
    "ember-cli-htmlbars": "^5.1.2",
    "tracked-maps-and-sets": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@ember/optional-features": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.24.0",
    "ember-cli-addon-docs": "^0.9.0",
    "ember-cli-addon-docs-esdoc": "^0.2.3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-document-title-northm": "^1.0.3",
    "ember-cli-fastboot": "^2.2.3",
    "ember-cli-fastboot-testing": "^0.4.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-composable-helpers": "^4.2.0",
    "ember-concurrency": "^1.1.7",
    "ember-concurrency-decorators": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-qunit-assert-helpers": "^0.2.2",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.25.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.8.0",
    "ember-truth-helpers": "^3.0.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-ember": "^9.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.5.1",
    "loader.js": "^4.7.0",
    "prettier": "^2.0.5",
    "qunit-dom": "^1.1.0",
    "qunit-wait-for": "^2.0.0",
    "standard-version": "^9.0.0",
    "testdouble": "^3.16.0",
    "testdouble-qunit": "^2.1.1"
  },
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/alexlafroscia/ember-steps",
  "license": "MIT",
  "author": "Alex LaFroscia <alex@lafroscia.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://alexlafroscia.github.io/ember-steps"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{md,json,yml}": "prettier --write"
  },
  "volta": {
    "node": "12.16.2",
    "yarn": "1.22.4"
  }
}
